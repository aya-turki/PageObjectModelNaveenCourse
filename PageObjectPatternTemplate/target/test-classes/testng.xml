<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression Suite" verbose="4"
       thread-count="4" parallel="tests">

    <listeners>
        <listener
                class-name="com.qa.java.listeners.ExtentReportListener" />
        <listener
                class-name="com.qa.java.listeners.TestAllureListener" />
        <listener
                class-name="com.qa.java.listeners.AnnotationTransformer" />
    </listeners>

    <test name="Open Cart Login Page Test">
        <classes>
            <class name="com.qa.java.PagesTest.LoginPageTest" />
        </classes>
    </test>

